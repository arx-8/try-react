{
  "name": "try-react",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "@emotion/core": "10.0.17",
    "@material-ui/core": "4.5.0",
    "@material-ui/icons": "4.4.3",
    "aws-amplify": "1.1.40",
    "aws-amplify-react": "2.4.4",
    "axios": "0.19.0",
    "dayjs": "1.8.16",
    "emotion-normalize": "10.1.0",
    "formik": "1.5.8",
    "immer": "4.0.0",
    "jest-emotion": "10.0.17",
    "lodash": "4.17.15",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "react-test-renderer": "16.10.2",
    "redux": "4.0.4",
    "redux-actions": "2.6.5",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-localstorage": "0.4.1",
    "redux-saga": "1.1.1",
    "redux-starter-kit": "0.7.0",
    "redux-thunk": "2.3.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.88.1",
    "typescript-fsa": "3.0.0",
    "typescript-fsa-reducers": "1.2.1",
    "ulid": "2.3.0",
    "use-react-router": "1.0.7",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.138",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.1",
    "@types/react-helmet": "5.0.10",
    "@types/react-redux": "7.1.4",
    "@types/react-router-dom": "4.3.5",
    "@types/react-test-renderer": "16.9.0",
    "@types/redux-actions": "2.6.1",
    "@types/redux-immutable-state-invariant": "2.1.1",
    "@types/redux-localstorage": "1.0.8",
    "@types/yup": "0.26.24",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "3.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "react-scripts": "3.1.2",
    "source-map-explorer": "2.1.0",
    "typescript": "3.5.3",
    "typesync": "0.6.1"
  },
  "scripts": {
    "analyze:run": "source-map-explorer 'build/static/js/*.js' --html misc/source-map-explorer.html",
    "analyze": "npm-run-all --sequential build analyze:run",
    "build": "react-scripts build",
    "eslint-print-config": "eslint --print-config .eslintrc.js > misc/eslint-print-config.json",
    "lint:eslint-dry": "eslint -c .eslintrc.js --ext .ts,.tsx,.js,.jsx src",
    "lint:eslint": "eslint -c .eslintrc.js --fix --ext .ts,.tsx,.js,.jsx src",
    "lint:tsc": "tsc --noEmit",
    "lint-dry": "npm-run-all --parallel --continue-on-error lint:eslint-dry lint:tsc",
    "lint": "npm-run-all --parallel --continue-on-error lint:eslint lint:tsc",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "typesync": "typesync --ignoredeps=dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
